{
    "orgName": "ScratchOrg",
    "edition": "Developer",
    "language": "en_US",
    "features": [
        "ContactsToMultipleAccounts",
        "Communities",
        "SiteForceContributor",
        "Sites",
        "Workflow",
        "DefaultWorkflowUser",
        "StateAndCountryPicklist",
        "FieldAuditTrail"
    ],
    "settings": {
        "apexSettings": {
            "enableDisableParallelApexTesting": true
        },
        "experienceBundleSettings": {
            "enableExperienceBundleMetadata": true
        },
        "communitiesSettings": {
            "enableNetworksEnabled": true
        },
        "languageSettings": {
            "enableTranslationWorkbench": true
        },
        "nameSettings": {
            "enableMiddleName": true,
            "enableNameSuffix": true
        },
        "surveySettings": {
            "enableSurvey": true
        },
        "securitySettings": {
            "sessionSettings": {
                "enableCacheAndAutocomplete": false
            }
        },
        "enhancedNotesSettings": {
            "enableEnhancedNotes": true
        }
    }
}
