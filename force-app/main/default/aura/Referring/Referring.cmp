                                                        ss.site.Site__r.BillingStateCode},
                                                        {!(!empty(ss.site.Principal_Investigator__r.Account.BillingAddress))?ss.site.Principal_Investigator__r.Account.BillingPostalCode:
                                                        ss.site.Site__r.BillingPostalCode}
                                                    </div>
                                                </aura:if>
                                            </div>

                                            <div class="slds-col slds-size--1-of-1 slds-medium-size--2-of-3 ssi-button">
                                                <button class="apollo-btn secondary ssi-btn" style="margin-top: 10px"
                                                    disabled="true">
                                                    {!ss.siteStatusLabel}
                                                </button>
                                            </div>
                                        </div>
                                    </aura:iteration>
                                </div>
                                <div
                                    class="slds-col slds-size_1-of-1 slds-medium-size_7-of-12 slds-order_1 slds-medium-order_2">
                                    <aura:if isTrue="{!v.markers}">
                                        <lightning:map mapMarkers="{!v.markers}"
                                            zoomLevel="{!if(empty(v.markers),1,10)}" />
                                    </aura:if>
                                    <!-- <c:RRGoogleMap accounts="{!v.studySiteMarkers}"/>-->
                                </div>
                            </div>
                        </aura:if>

                        <button class="apollo-btn primary" onclick="{!c.doGoFindStudySites}">
                            {!$Label.c.BTN_Find_study_sites}
                        </button>
                        <button class="apollo-btn tertiary" onclick="{!c.doGoHome}">
                            <aura:if isTrue="{!v.patientVeiwRedirection}">
                                {!$Label.c.RH_RP_Back_to_My_Patients}
                                <aura:set attribute="else">
                                    {!$Label.c.PG_MRR_BTN_Back_to_My_Studies}
                                </aura:set>
                            </aura:if>
                        </button>
                    </div>
                </div>
            </aura:if>

            <aura:if isTrue="{!v.currentState == 'No Longer Interested'}">
                <div class="rr-width-limiter">
                    <div class="big-alert" style="padding: 10px 0">
                        <div class="message">
                            {!$Label.c.PG_Ref_L_Thank_you_for_your_time_and_interest_in_this_study}
                        </div>
                    </div>
                    <div class="q-failed">
                        <span>{!$Label.c.PG_Ref_L_Thank_you_for_having_a_discussion_with_your_patient_about_clinical_stud}</span>
                        <button class="apollo-btn primary" onclick="{!c.doReferrAnotherPatient}">
                            <!--                                disabled="{!v.actions.refer.disabled}"-->
                            <!--                                title="{!v.actions.refer.disableReason}"-->
                            {!$Label.c.BTN_Refer_Another_Patient_to_This_Study}
                        </button>
                        <button class="apollo-btn tertiary" onclick="{!c.doGoHome}">
                            <aura:if isTrue="{!v.patientVeiwRedirection}">
                                {!$Label.c.RH_RP_Back_to_My_Patients}
                                <aura:set attribute="else">
                                    {!$Label.c.PG_MRR_BTN_Back_to_My_Studies}
                                </aura:set>
                            </aura:if>
                        </button>
                    </div>
                </div>
            </aura:if>

            <aura:if isTrue="{!v.currentState == 'Had Discussion, Not Interested'}">
                <div class="rr-width-limiter">
                    <div class="big-alert" style="padding: 10px 0">
                        <div class="message">{!$Label.c.PG_Ref_MSG_Response_Saved}</div>
                    </div>
                    <div class="q-failed">
                        <span>{!$Label.c.PG_Ref_L_Thank_you_for_having_a_discussion_with_your_patient_about_clinical_stud}</span>
                        <button class="apollo-btn primary" onclick="{!c.doReferrAnotherPatient}">
                            <!--disabled="{!v.actions.refer.disabled}"
              title="{!v.actions.refer.disableReason}"-->
                            {!$Label.c.BTN_Refer_Another_Patient_to_This_Study}
                        </button>
                    </div>
                </div>
            </aura:if>

            <aura:if isTrue="{!v.currentState == 'Refer Success'}">
                <div class="ref-alert pass">
                    <c:RRIcon iconName="icon-check-circle" />
                    <div class="main-title">{!$Label.c.PG_Ref_L_Referral_Sent}</div>
                    <div class="rr-sub-title rr-green">
                        {!$Label.c.PG_Ref_L_Thank_you_for_your_time_and_interest_in_this_study}
                    </div>
                    <div class="rr-sub-text rr-dark-gray">{!$Label.c.PG_Ref_ST1_Email_sent}</div>
                    <div class="rr-sub-text rr-dark-gray">{!$Label.c.PG_Ref_ST2_Email_sent}</div>
                    <aura:if isTrue="{!v.patientVeiwRedirection}">
                        <aura:set attribute="else">
                            <button class="apollo-btn primary" onclick="{!c.doReferrAnotherPatient}">
                                <!--disabled="{!v.actions.refer.disabled}"
          title="{!v.actions.refer.disableReason}"-->
                                {!$Label.c.BTN_Refer_Another_Patient_to_This_Study}
                            </button>
                        </aura:set>
                    </aura:if>
                    <button class="apollo-btn tertiary" onclick="{!c.doGoHome}">
                        <aura:if isTrue="{!v.patientVeiwRedirection}">
                            {!$Label.c.RH_RP_Back_to_My_Patients}
                            <aura:set attribute="else">
                                {!$Label.c.PG_MRR_BTN_Back_to_My_Studies}
                            </aura:set>
                        </aura:if>
                    </button>
                </div>
            </aura:if>

            <div class="rr-width-limiter side-padding">
                <aura:if
                    isTrue="{!AND(AND(v.mrrResult != 'Fail',v.mrrResult != 'Pass'),v.currentState == 'Search PE')}">
                    <div class="slds-grid slds-wrap" style="align-items: stretch">
                        <div
                            class="{!'slds-col slds-size_1-of-1 slds-large-size_4-of-12 ' + if(v.isRTL,'left-columnRTL','left-column')}">
                            <div class="rr-card">
                                <c:RRIcon iconName="chart-review-sent"
                                    class="{!v.isRTL? 'rr-card-icoRTL':'rr-card-ico'}" />
                                <div class="rr-secondary-text" style="padding-top: 4px">
                                    {!$Label.c.PG_MRR_L_Please_complete_medical_record_review_for}
                                </div>
                                <aura:if isTrue="{!!empty(v.hcpEnrollment)}">
                                    <div class="rr-primary-text">
                                        <aura:if
                                            isTrue="{!or(v.hcpEnrollmentParticipantName==undefined,v.hcpEnrollmentParticipantName==null)}">
                                            {!v.hcpEnrollment.HCP_Contact__r.Salutation ?
                                            v.hcpEnrollment.HCP_Contact__r.Salutation + ' ' : ''}
                                            {!v.hcpEnrollment.HCP_Contact__r.Name}
                                            <aura:set attribute="else">
                                                {!v.hcpEnrollmentParticipantName}
                                            </aura:set>
                                        </aura:if>
                                    </div>
                                    <div class="rr-secondary-text">
                                        {!$Label.c.PG_DBPI_at + ' ' +
                                        v.hcpEnrollment.Study_Site__r.Name}
                                    </div>
                                    <aura:set attribute="else">
                                        <aura:if isTrue="{!!empty(v.hcpContact)}">
                                            <div class="rr-primary-text">
                                                <aura:if
                                                    isTrue="{!or(v.hcpContactParticipant==undefined,v.hcpContactParticipant==null)}">
                                                    {!v.hcpContact.Salutation} {!v.hcpContact.Name}
                                                    <aura:set attribute="else">
                                                        {!v.hcpContactParticipantSal}
                                                        {!v.hcpContactParticipantName}
                                                    </aura:set>
                                                </aura:if>
                                            </div>
                                        </aura:if>
                                    </aura:set>
                                </aura:if>
                            </div>
                            <br />
                            <div class="rr-secondary-text" style="padding-top: 20px">
                                {!$Label.c.PG_MRR_Note_Remember}
                            </div>
                        </div>
                        <div
                            class="{!'slds-col slds-size_1-of-1 slds-large-size_8-of-12 ' + if(v.isRTL,'right-columnRTL','right-column')}">
                            <c:SearchParticipantEnrollment aura:id="searchPECmp" trialId="{!v.trialId}"
                                trial="{!v.trial}" searchData="{!v.searchData}" searchResult="{!v.searchResult}"
                                mrrResult="{!v.mrrResult}" preSurvey="{!v.preMrrSurvey}"/>
                        </div>
                    </div>
                </aura:if>
            </div>
        </div>

        <div class="rr-width-limiter side-padding">
            <c:CommunityFooter />
        </div>
    </aura:if>

    <c:builderStub aura:id="builderStub" />
</aura:component>