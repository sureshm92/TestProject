<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PI_Hold</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISCHANGED(Override_PI_Referral_Status__c ),
(ISPICKVAL(PRIORVALUE(Override_PI_Referral_Status__c ), &quot;On Hold&quot;)),ISPICKVAL(Override_Study_Site_Recruitment_Status__c, &quot;On Hold&quot;) )</errorConditionFormula>
    <errorMessage>This Override PI Recruitment Status value cannot be changed</errorMessage>
</ValidationRule>
