<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expire_Not_Negative</fullName>
    <active>true</active>
    <errorConditionFormula>AND
   (NOT( ISBLANK( Expires_After_Days__c ) ),
    Expires_After_Days__c &lt; 0
   )</errorConditionFormula>
    <errorMessage>Please enter zero or positive value on &quot;Number of Days Before Survey Expires&quot;</errorMessage>
</ValidationRule>
