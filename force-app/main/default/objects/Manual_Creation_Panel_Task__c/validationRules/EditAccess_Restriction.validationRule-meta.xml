<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EditAccess_Restriction</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(ISPICKVAL(PRIORVALUE( Status__c ), &apos;Active&apos; ))</errorConditionFormula>
    <errorMessage>Records with Active status only have the edit access</errorMessage>
</ValidationRule>
