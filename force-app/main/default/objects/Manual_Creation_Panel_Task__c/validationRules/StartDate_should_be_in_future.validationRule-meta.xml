<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StartDate_should_be_in_future</fullName>
    <active>false</active>
    <description>Start Date should be in future.</description>
    <errorConditionFormula>ISCHANGED(Start_Date__c) &amp;&amp; Start_Date__c  &lt;=  TODAY()</errorConditionFormula>
    <errorDisplayField>Start_Date__c</errorDisplayField>
    <errorMessage>Value must be Greater than Today.</errorMessage>
</ValidationRule>
