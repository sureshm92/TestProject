<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Study_Site_On_Hold</fullName>
    <active>false</active>
    <errorConditionFormula>AND( ISPICKVAL(Study_Site__r.Override_Study_Site_Recruitment_Status__c, &quot;On Hold&quot;), (ISPICKVAL(Referral_Source__c, &quot;ePR&quot;)= FALSE))</errorConditionFormula>
    <errorMessage>Study Site is On Hold, please select an active Study Site</errorMessage>
</ValidationRule>
