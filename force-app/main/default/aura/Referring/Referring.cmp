 - Created by Leonid Bartenev
 -->

<aura:component description="Referring"
    implements="forceCommunity:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,lightning:isUrlAddressable,flexipage:availableForAllPageTypes"
    controller="ReferringRemote" extends="c:CommunityContext" access="global">
    <ltng:require scripts="{!$Resource.rr_community_js}" afterScriptsLoaded="{!c.doInit}" />
    <lightning:navigation aura:id="navService" />
    <!-- attributes: -->
    <aura:attribute name="trialId" type="String" />
    <aura:attribute name="siteId" type="String" />
    <aura:attribute name="trial" type="Object" />
    <aura:attribute name="preScreenerSurvey" type="Object" />	
    <aura:attribute name="preMrrSurvey" type="Object" />	
    <aura:attribute name="preScreenerResponse" type="Object"/>
    <aura:attribute name="pEnrollment" type="Object" />
    <aura:attribute name="mailingCountryCode" type="String" />
    <aura:attribute name="hcpeId" type="String" />
    <aura:attribute name="hcpEnrollment" type="Object" />
    <aura:attribute name="pendingPEnrollments" type="List" />
    <aura:attribute name="studySites" type="List" />
    <aura:attribute name="studySitesPending" type="List" />
    <aura:attribute name="studySiteMarkers" type="List" />
    <aura:attribute name="participant" type="Object" />
    <aura:attribute name="participantToInsert" type="Object" />
    <aura:attribute name="delegateParticipant" type="Object" />
    <aura:attribute name="emailRepeat" type="String" />
    <aura:attribute name="emailDelegateRepeat" type="String" />
    <aura:attribute name="allRequiredCompleted" type="Boolean" default="false" />
    <aura:attribute name="agreeShare" type="Boolean" default="false" />
    <aura:attribute name="agreePolicy" type="Boolean" default="false" />
    <aura:attribute name="actions" type="Object" access="private" />
    <aura:attribute name="genders" type="List" />
    <aura:attribute name="phoneTypes" type="List" />
    <aura:attribute name="todayDate" type="Date" />
    <aura:attribute name="markers" type="Object[]" />
    <aura:attribute name="accessUserLevel" type="String" />
    <aura:attribute name="delegateDuplicateInfo" type="Object" />
    <aura:attribute name="useThisDelegate" type="Boolean" default="true" />
    <aura:attribute name="emailInstance" type="String" />
    <aura:attribute name="hadDiscussionDocumented" type="Boolean" default="false" />
    <aura:attribute name="showMRRButton" type="Boolean" />

    <aura:attribute name="primaryDelegateFirstname" type="String" />
    <aura:attribute name="primaryDelegateLastname" type="String" />
    <aura:attribute name="primaryDelegateEmail" type="String" />
    <aura:attribute name="primaryDelegatePhonenumber" type="String" />
    <aura:attribute name="primaryDelegatePhonetype" type="String" />
    <aura:attribute name="primaryDelegateYob" type="String" />
    <aura:attribute name="isDelegateCertify" type="boolean" default="false" />
    <aura:attribute name="birthMonth" type="String" />
    <aura:attribute name="yearofBirth" type="String" />
    <aura:attribute name="birthdate" type="String" />
    <aura:attribute name="participantDate" type="date" />
    <aura:attribute name="studySiteFormat" type="String" />
    <aura:attribute name="ageOptions" type="List" />
    <aura:attribute name="selectedAge" type="string" default="" />

    <aura:attribute name="perecordid" type="string" access="global" />
    <aura:attribute name="patientVeiwRedirection" type="Boolean" default="false" access="global" />

    <aura:attribute name="steps" type="String[]" default="[]" />
    <aura:attribute name="currentStep" type="String" default="{!$Label.c.PG_Ref_Step_Contact_Info}" />
    <aura:attribute name="currentState" type="String" description="Name of current state ('Select Source', 'Screening')"
        default="Screening" />
    <aura:attribute name="hadDiscussion" type="Boolean" />
    <aura:attribute name="stillInterested" type="Boolean" />
    <aura:attribute name="frameHeight" type="Integer" default="500" />
    <aura:attribute name="authorizationForm" type="Boolean" default="false" />
    <aura:attribute name="authRequired" type="Boolean" />
    <aura:attribute name="fileRequired" type="Boolean" default="true" />

    <aura:attribute name="searchResult" type="Object" />
    <aura:attribute name="searchData" type="Object" />
    <aura:attribute name="mrrResult" type="String" />

    <aura:attribute name="counries" type="List" />
    <aura:attribute name="selectedCountry" type="String" />
    <aura:attribute name="states" type="List" />
    <aura:attribute name="selectedState" type="String" />
    <aura:attribute name="statesByCountyMap" type="Object" />
    <aura:attribute name="yob" type="List" />
    <!--added by sumit -->
    <aura:attribute name="attestAge" type="Boolean" default="false" />
    <!--added by sumit -->
    <aura:attribute name="isAdultDel" type="Boolean" />
    <!--added by sumit -->
    <aura:attribute name="yobBlankErrMsg" type="Boolean" default="false" />
    <!--added by sumit -->
    <aura:attribute name="delNotAdultErrMsg" type="Boolean" default="false" />
    <!--added by sumit -->
    <aura:attribute name="errorMessage" type="String" default="" />
    <aura:attribute name="isNewPrimaryDelegate" type="Boolean" default="true" />
    <!--added by sumit -->

    <aura:attribute name="needsGuardian" type="Boolean" default="false" />
    <aura:attribute name="enableGuardian" type="Boolean" default="true" />
    <!-- REF-3070-->
    <aura:attribute name="fileName" type="String" />
    <aura:attribute name="disableFile" type="Boolean" default="false" />
    <aura:attribute name="contentDocId" type="String" />
    <aura:attribute name="contentDoc" type="String" />
    <aura:attribute name="openmodel" type="boolean" default="false" />
    <aura:attribute name="closeModel" type="boolean" default="false" />

    <aura:attribute name="days" type="List" />
    <aura:attribute name="months" type="List" />
    <aura:attribute name="years" type="List" />
    <aura:attribute name="pday" type="string" default="null" />
    <aura:attribute name="pmonth" type="string" default="null" />
    <aura:attribute name="pyear" type="string" default="null" />
    <aura:attribute name="doNext" type="Boolean" default="false" />
    <aura:attribute name="delegateExist" type="Boolean" default="false" />

    <aura:attribute name="hasGaurdian" type="Boolean" default="false" />
    <aura:attribute name="delegateValueRemoved" type="Boolean" default="false" />
    <aura:attribute name="discussiondocumented" type="Boolean" default="false" />
    <aura:attribute name="mrrExist" type="Boolean" default="false" />
    <aura:attribute name="myStudies" type="Boolean" default="false" />
    <aura:attribute name="PeID" type="string" />
    <aura:attribute name="toggleAge" type="Boolean" default="true" />
    <!--RH-8091-->
    <aura:attribute name="confirmConsent" type="Boolean" default="false" />
    <aura:attribute name="isConfirmConsentNeeded" type="Boolean" default="false" />
    <!--RH-8091-->

    <!-- handlers: -->
    <aura:handler event="c:EventCommunityInitialized" action="{!c.doInit}" />
    <aura:handler name="change" value="{!v.mrrResult}" action="{!c.doStartPreScreening}" />
    <aura:handler name="change" value="{!v.participant.Mailing_Country_Code__c}" action="{!c.doCountryChange}" />
    <!--<aura:handler name="change" value="{!v.participant.Mailing_State_Code__c}" action="{!c.doCheckfields}"/>-->
    <aura:handler name="ddMessageButtonClick" event="c:DDMessageEvent" action="{!c.approveDelegate}" />
    <aura:method name="checkdobMethod" action="{!c.doCheckDateOfBith}" />
    <aura:method name="dobChangeMethod" action="{!c.doDOBChange}" />
    <aura:method name="checkdoneeedgaurdian" action="{!c.doNeedsGuardian}" />
    <!--<c:ExceptionHandlerPopup aura:id="exceptionHandler" />-->

    <aura:attribute name="futureDate" type="Boolean" />
    <aura:attribute name="futureDateDDErr" type="String" />
    <aura:attribute name="futureDateMMErr" type="String" />
    <c:RRSpinner aura:id="mainSpinner" size="medium" fixed="true" />

    <aura:if isTrue="{!!empty(v.trial)}">
        <!-- top page bar -->
        <c:RRTopBar backLabel="{!$Label.c.PG_MRR_L_Back_to_Study_Workspace}" backPage=""
            smallTitle="{!if(!empty(v.trial),v.trial.Study_Code_Name__c,'')}"
            bigTitle="{!$Label.c.DLIT_L_Pre_eligibility_screening}">
            <aura:if isTrue="{!AND(OR(v.mrrResult == 'Fail',v.mrrResult == 'Pass'),v.currentState == 'Search PE')}">
                <div class="rr-body-content"
                    style="{!if(v.isRTL, 'position: relative; direction: rtl;','position: relative;')}">
                    <div class="rr-width-limiter side-padding">

                        <aura:if isTrue="{!v.mrrResult == 'Fail'}">
                            <div class="mrr-alert fail">
                                <c:RRIcon iconName="icon-close-circle" />
                                <div class="main-title" id="anchor">
                                    {!v.searchResult.pe.Participant_Name__c}
                                    &nbsp;{!$Label.c.PG_MRR_R_Failed}
                                </div>
                                <button class="apollo-btn primary" onclick="{!c.doClearForm}">
                                    {!$Label.c.PG_MRR_BTN_Complete_another_Medical_Record_Review}
                                </button>
                                <button class="apollo-btn tertiary" onclick="{!c.doGoPatientsTab}">
                                    {!$Label.c.RH_RP_Back_to_My_Patients}
                                </button>
                            </div>
                        </aura:if>

                        <aura:if isTrue="{!v.mrrResult == 'Pass'}">
                            <div class="mrr-alert pass">
                                <c:RRIcon iconName="icon-check-circle" />
                                <div class="main-title" id="anchor">
                                    {!v.searchResult.pe.Participant_Name__c}
                                    &nbsp;{!$Label.c.PG_MRR_R_Passed}
                                </div>
                                <button class="apollo-btn primary" onclick="{!c.doReferPatient}"
                                    disabled="{!OR(v.accessUserLevel == 'Level 2' , v.accessUserLevel == 'Level 3')}"
                                    title="{!v.actions.referToThisStudy.disableReason}">
                                    {!$Label.c.PG_MRR_BTN_Refer_Patient_to_This_Study}
                                </button>
                                <button class="apollo-btn secondary" onclick="{!c.doStartMRR}">
                                    {!$Label.c.PG_MRR_BTN_Complete_another_Medical_Record_Review_for_this_Study}
                                </button>
                                <button class="apollo-btn tertiary" onclick="{!c.doGoPatientsTab}">
                                    {!$Label.c.RH_RP_Back_to_My_Patients}
                                </button>
                            </div>
                        </aura:if>
                    </div>
                </div>
            </aura:if>
            <aura:if isTrue="{!v.currentState == 'Screening'}">
                <div class="buttons-area">
                    <div class="rr-width-limiter buttons-block">
                        <div class="apollo-btn secondary" onclick="{!c.doReferrAnotherPatient}"
                            style="{!if(v.isRTL,'margin-left: 1%;','')}">
                            <div>{!$Label.c.BTN_Stat_Over}</div>
                        </div>
                        <div class="apollo-btn secondary" onclick="{!c.doNoLongerInterested}">
                            <div>{!$Label.c.BTN_Patient_no_longer_interested}</div>
                        </div>
                    </div>
                </div>
                <div class="path-area">
                    <div class="rr-width-limiter side-padding">
                        <c:RRPath steps="{!v.steps}" currentStep="{!v.currentStep}" />
                    </div>
                </div>
            </aura:if>
        </c:RRTopBar>

        <aura:if isTrue="{!v.currentState == 'Screening'}">
            <div class="rr-body-content" style="height: 74px"></div>
        </aura:if>

        <!-- page content -->
        <div class="rr-body-content"
            style="{!'min-height: 500px; position: relative; padding-bottom: 100px;padding-top:25px' + if(v.isRTL,'/* @noflip */  direction: rtl;','')}">
            <!-- Select source -->
            <aura:if isTrue="{!v.currentState == 'Select Source'}">
                <div class="{!v.isRTL? 'rr-width-limiterRTL side-padding':'rr-width-limiter side-padding'}">
                    <div class="{!v.isRTL? 'pe-large-headerRTL':'pe-large-header'}">
                        {!$Label.c.PG_Ref_L_Please_select_your_referral_source}
                    </div>
                    <div class="pe-panel padding" style="padding-bottom: 30px">
                        <c:RRIcon iconName="referral" />
                        <div class="title">{!$Label.c.PG_Ref_L_New_Patient}</div>
                        <div class="rr-sub-title">
                            {!$Label.c.PG_Ref_L_Please_complete_Medical_Record_Review_first}
                        </div>
                        <button class="apollo-btn primary" onclick="{!c.doGoToMedicalRecordReview}">
                            <!--disabled="{!v.actions.medicalRecordReview.disabled}"
          title="{!v.actions.medicalRecordReview.disableReason}"-->
                            {!$Label.c.BTN_Start_Pre_eligibility_Screening}
                        </button>
                    </div>
                    <aura:if
                        isTrue="{!!empty(v.pendingPEnrollments) &amp;&amp; v.accessUserLevel != 'Level 2' &amp;&amp; v.accessUserLevel != 'Level 3'}">
                        <div class="between-panels">{!$Label.c.PG_Ref_L_OR}</div>
                        <div class="pe-panel padding" style="margin-bottom: 50px">
                            <c:RRIcon iconName="chart-review-sent" />
                            <div class="title">
                                {!$Label.c.PG_Ref_L_Select_from_Medical_Record_Review_Log}
                            </div>
                            <div class="mrr-items" style="margin-top: 30px">
                                <aura:iteration items="{!v.pendingPEnrollments}" var="pe">
                                    <div class="mrr-item">
                                        <div class="mrr-item-content">
                                            <div>
                                                <div class="mrr-status-content">
                                                    <div class="mrr-item-id">{!pe.Name}</div>
                                                    <c:ReviewStatus pEnrollment="{!pe}" />
                                                </div>

                                                <div class="sub-line" style="{!if(v.isRTL, 'float: right;','')}">
                                                    {!pe.Patient_ID__c}
                                                </div>
                                            </div>
                                            <div class="{!v.isRTL? 'mrr-item-btnsRTL':'mrr-item-btns'}">
                                                <!--<button class="apollo-btn tertiary">View Medical Record Result</button> todo delete-->
                                                <button class="apollo-btn primary" id="{!pe.Id}"
                                                    onclick="{!c.doReferSelectedPE}">
                                                    <!--disabled="{!v.actions.referToThisStudy.disabled}"
                                  title="{!v.actions.referToThisStudy.disableReason}"-->
                                                    <!--{!v.actions.referToThisStudy.label}-->
                                                    {!$Label.c.BTN_Refer_to_this_study}
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </aura:iteration>
                            </div>
                        </div>
                    </aura:if>
                </div>
            </aura:if>

            <!-- Screening -->
            <aura:if isTrue="{!v.currentState == 'Screening'}">
                <aura:if isTrue="{!v.currentStep == $Label.c.PG_Ref_Step_Discussion}">
                    <div class="rr-width-limiter side-padding"
                        style="{!if(v.isRTL, '/* @noflip */  direction: rtl;','')}">
                        <div class="big-alert">
                            <c:RRIcon iconName="communication" />
                            <div class="message">
                                {!$Label.c.PG_Ref_L_Have_you_had_a_discussion_with_your_patient_about_participating_in_a_cl}
                            </div>
                            <div class="buttons">
                                <button class="{!'apollo-btn tertiary ' + if(v.hadDiscussion == true, ' selected', '')}"
                                    style="{!if(v.isRTL, 'margin-left: 1%;','')}" onclick="{!c.doHadDiscussion}"
                                    disabled="{!!empty(v.hadDiscussion)}">
                                    {!$Label.c.BTN_Yes}
                                </button>
                                <button
                                    class="{!'apollo-btn tertiary ' + if(v.hadDiscussion == false, ' selected', '')}"
                                    onclick="{!c.doNoHadDiscussion}" disabled="{!!empty(v.hadDiscussion)}">
                                    {!$Label.c.BTN_No}
                                </button>
                            </div>
                        </div>
                        <aura:if isTrue="{!v.hadDiscussion == false}">
                            <div class="pe-panel padding big-alert">
                                <div class="message">
                                    {!$Label.c.PG_Ref_L_Please_first_have_a_meaningful_discussion_with_your_patient_about_parti}
                                </div>
                            </div>
                        </aura:if>
                        <aura:if isTrue="{!and(!empty(v.hadDiscussion), v.hadDiscussion)}">
                            <div class="rr-width-limiter side-padding"></div>
                            <div class="big-alert">
                                <c:RRIcon iconName="verbal-agreement" />
                                <div class="message">
                                    {!$Label.c.PG_Ref_L_Is_the_patient_still_interested_in_participating_in_this_study}
                                </div>
                                <div class="buttons">
                                    <button
                                        class="{!'apollo-btn tertiary ' + if(v.hadDiscussionDocumented == true, ' selected', '')}"
                                        onclick="{!c.doStillInterested}" style="{!if(v.isRTL, 'margin-left: 1%;','')}"
                                        disabled="{!v.hadDiscussionDocumented}">
                                        {!$Label.c.BTN_Yes}
                                    </button>
                                    <button class="apollo-btn tertiary" onclick="{!c.doHadDiscussionNotInterested}"
                                        disabled="{!v.hadDiscussionDocumented}">
                                        {!$Label.c.BTN_No}
                                    </button>
                                </div>
                            </div>
                            <!--New Changes-->
                            <aura:if isTrue="{!v.hadDiscussionDocumented}">
                                <div class="big-alert">
                                    <div class="" style="
                                            font-size: 23px;
                                            font-weight: 600;
                                            font-family: Proxima Nova;
                                        ">
                                        {!$Label.c.RH_RP_documented_the_discussion}
                                        <div>
                                            <input type="checkbox" disabled="{!v.discussiondocumented}" id="terms"
                                                name="terms" onclick="{!c.doDiscussionDocumented}" value="{!v.chkTerms}"
                                                onchange="{!c.termsChanged}" />
                                            <label for="terms" style="
                                                    font-size: 18px;
                                                    color: black;
                                                    font-weight: 550;
                                                    font-family: Proxima Nova;
                                                ">&nbsp;{!$Label.c.RH_RP_discussion_noted}</label>
                                        </div>
                                    </div>
                                </div>
                            </aura:if>
                            <!---New Changes End -->
                        </aura:if>
                        <!-- Added for REF-3229-->
                        <aura:if isTrue="{!and(!empty(v.authorizationForm), v.authorizationForm)}">
                            <div class="rr-width-limiter side-padding"></div>
                            <div class="big-alert">
                                <c:RRIcon iconName="verbal-agreement" />
                                <div class="message">{!$Label.c.PG_Ref_L_Authorization_Form}</div>
                                <div>
                                    {!$Label.c.PG_Ref_L_Download_Authorization_Form_Template_First_Part}
                                    <a href="{!v.contentDoc.baseURL + '/sfc/servlet.shepherd/document/download/'+ v.contentDoc.attachments[0].Id +'?operationContext=S1'}"
                                        target="_blank" download="{!v.contentDoc.attachments.Title}"
                                        style="padding-left: 5px; padding-right: 5px">
                                        {!$Label.c.PG_Ref_L_Download_Authorization_Form_Template_Link}
                                    </a>
                                    {!$Label.c.PG_Ref_L_Download_Authorization_Form_Template_Last_Part}
                                </div>
                                <div class="buttons">
                                    <button class="apollo-btn tertiary" onclick="{!c.doNext}"
                                        style="{!if(v.isRTL, 'margin-left: 1%;','')}">
                                        {!$Label.c.BTN_Next}
                                    </button>
                                </div>
                            </div>
                        </aura:if>
                        <aura:if isTrue="{!v.doNext}">
                            <div class="buttons" style="text-align: center">
                                <button class="apollo-btn tertiary" onclick="{!c.doNext}"
                                    style="{!if(v.isRTL, 'margin-left: 1%;','')}">
                                    {!$Label.c.BTN_Next}
                                </button>
                            </div>
                        </aura:if>
                    </div>
                </aura:if>

                <!-- Site selection -->
                <aura:if isTrue="{!v.currentStep == $Label.c.PG_Ref_Step_Site_Selection}">
                    <div class="rr-width-limiter side-padding" style="padding-bottom: 25px">
                        <div class="big-alert" style="padding: 10px 0">
                            <div class="message">
                                {!$Label.c.PG_Ref_L_Please_select_referred_study_site_s_to_refer_your_patient_to}
                            </div>
                        </div>
                        <div class="slds-grid slds-wrap pe-panel"
                            style="{!if(v.isRTL, '/* @noflip */  direction: rtl;','')}">
                            <div
                                class="slds-col slds-size_1-of-1 slds-medium-size_5-of-12 slds-order_2 slds-medium-order_1">
                                <!-- todo -->
                                <aura:iteration items="{!v.studySites}" var="ss" indexVar="index">
                                    <!--                                    <div class="sdas-footer-info">-->
                                    <!--                                        <div class="name">1. {!ss.name}</div>-->
                                    <!--                                        <div class="address">-->
                                    <!--                                            <div class="address-1">{!ss.addrStreet}</div>-->
                                    <!--                                            <div class="address-3">{!ss.addrCity}</div>-->
                                    <!--                                        </div>-->
                                    <!--                                        <button data-hcpe-id="{!ss.hcpeId}" class="apollo-btn tertiary" style="margin-top: 10px" onclick="{!c.doSelectSite}">-->
                                    <!--                                            {!$Label.c.BTN_Select_as_Preferred_Site}-->
                                    <!--                                        </button>-->
                                    <!--                                    </div>-->
                                    <div class="slds-grid slds-wrap slds-p-around--medium">
                                        <div class="slds-col slds-size--1-of-1 slds-medium-size--2-of-3">
                                            <div class="ssi-site-name" style="{!if(v.isRTL, 'text-align: right;','')}">
                                                {!index+1 +'. '+ ss.site.Name}
                                            </div>
                                            <div class="ssi-pi-name" style="{!if(v.isRTL, 'text-align: right;','')}">
                                                {!ss.site.Principal_Investigator__r.Salutation + ' '
                                                + ss.site.Principal_Investigator__r.Name}
                                            </div>
                                        </div>
                                        <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-3">
                                            <div class="ssi-site-type" style="{!if(v.isRTL, 'text-align: left;','')}">
                                                {!if(!empty(ss.siteType),ss.siteTypeLabel + ' ' +
                                                $Label.c.PG_SFS_L_Site,'')}
                                            </div>
                                            <aura:if isTrue="{!!empty(ss.formatedDistance)}">
                                                <div class="ssi-site-distance"
                                                    style="{!if(v.isRTL, 'text-align: left;','')}">
                                                    <aura:unescapedHtml value="{!ss.formatedDistance}" />
                                                </div>
                                            </aura:if>
                                        </div>
                                        <div class="slds-col slds-size--1-of-1 slds-medium-size--1-of-3">
                                            <aura:if isTrue="{!!empty(ss.formatedDistance)}">
                                                <div class="ssi-address"
                                                    style="{!if(v.isRTL, 'text-align: right;','')}">
                                                    {!(!empty(ss.site.Site__r.BillingAddress))?ss.site.Site__r.BillingStreet
                                                    :
                                                    ss.site.Principal_Investigator__r.Account.BillingStreet}
                                                    <br />
                                                    {!(!empty(ss.site.Site__r.BillingAddress))?
                                                    ss.site.Site__r.BillingCity?
                                                    ss.site.Site__r.BillingCity : '' :
                                                    ss.site.Principal_Investigator__r.Account.BillingCity?
                                                    ss.site.Principal_Investigator__r.Account.BillingCity
                                                    : ''}
                                                    {!(!empty(ss.site.Site__r.BillingAddress))?
                                                    ss.site.Site__r.BillingStateCode? ',
                                                    '+ss.site.Site__r.BillingStateCode : '' :
                                                    ss.site.Principal_Investigator__r.Account.BillingStateCode?
                                                    ', ' +
                                                    ss.site.Principal_Investigator__r.Account.BillingStateCode
                                                    : ''}
                                                    {!(!empty(ss.site.Site__r.BillingAddress))?
                                                    ss.site.Site__r.BillingPostalCode? ',
                                                    '+ss.site.Site__r.BillingPostalCode : '' :
                                                    ss.site.Principal_Investigator__r.Account.BillingPostalCode?
                                                    ', ' +
                                                    ss.site.Principal_Investigator__r.Account.BillingPostalCode
                                                    : ''}
                                                    <!-- ss.site.Site__r.BillingStateCode ? ',' + ss.site.Site__r.BillingStateCode : '' +
                                          ss.site.Site__r.BillingPostalCode ? ',' + ss.site.Site__r.BillingPostalCode : '') :
                                          (ss.site.Principal_Investigator__r.Account.BillingCity? ',' +  ss.site.Principal_Investigator__r.Account.BillingCity : '' +
                                          ss.site.Principal_Investigator__r.Account.BillingStateCode ? ',' + ss.site.Principal_Investigator__r.Account.BillingStateCode : '' +
