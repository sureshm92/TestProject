<aura:component implements= "force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,lightning:actionOverride"
                access="global" controller="ServiceLookupController">
	
     <aura:attribute name="selectedRecord" type="Object" access="public" default="" description="selected Record Details" />
   
  
    
    <lightning:card title="Service Resource">
      <div class="slds-col--padded-large slds-large-size--1-of-2 slds-medium-size--1-of-2 slds-small-size--1-of-1 slds-x-small-size--1-of-1 slds-m-bottom--medium">
            <div class="slds-form-element__control">Principal Investigator</div> 
        </div>
                    <div class="slds-col--padded-large slds-large-size--1-of-2 slds-medium-size--1-of-2 slds-small-size--1-of-1 slds-x-small-size--1-of-1 slds-m-bottom--medium">
            <div class="slds-form-element__control">
    <c:ServiceLookupcmp objectName="User" fieldName="Name" label="" iconName="standard:contact" placeholder="Enter Value" selectedRecord="{!v.selectedRecord}" />
            </div>
         </div>
       
          <div class="slds-col--padded-large slds-grid slds-grid--align-center">                    
                <button class="slds-button slds-button--brand" onclick="{!c.save}">Save</button>
                <button class="slds-button slds-button--destructive" onclick="{!c.back}">Cancel</button>
            </div>
          <!--<lightning:button label="Save" onclick="{!c.save}" class="apollo-btn secondary"/>-->
    </lightning:card>
    
   
    
 
    </aura:component>