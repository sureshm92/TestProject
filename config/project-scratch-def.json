{
    "orgName": "ScratchOrg",
    "edition": "Developer",
    "language": "en_US",
    "features": [
        "ContactsToMultipleAccounts",
        "Communities",
        "SiteForceContributor",
        "Sites",
        "SurveyCreatorUser",
        "Workflow",
        "DefaultWorkflowUser",
        "StateAndCountryPicklist"
    ],
    "settings": {
        "experienceBundleSettings": {
            "enableExperienceBundleMetadata": true
        },
        "communitiesSettings": {
            "enableNetworksEnabled": true
        },
        "languageSettings": {
            "enableTranslationWorkbench": true
        },
        "nameSettings": {
            "enableMiddleName": true,
            "enableNameSuffix": true
        },
        "surveySettings": {
            "enableSurvey": true
        }
    }
}