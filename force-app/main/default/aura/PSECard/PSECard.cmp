<aura:component description="PSECard">

    <!-- attributes: -->
    <aura:attribute name="pe" type="Object"/>
    <aura:attribute name="steps" type="List"/>
    <aura:attribute name="userMode" type="String"/>

    <!-- component body:-->
    <div class="rr-white-panel padding" style="position: relative">
        <c:ParticipantCardHeader userMode="{!v.userMode}" pe="{!v.pe}"/>
        <c:ParticipantCardFields userMode="{!v.userMode}" pe="{!v.pe}"/>
        <c:PSEPath steps="{!v.steps}" pe="{!v.pe}"/>
    </div>

</aura:component>
